<?xml version='1.0'?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:msxsl="urn:schemas-microsoft-com:xslt"><xsl:output method="html"/><xsl:template match="/">	<script type="text/javascript" src="/Assets/Common/Js/OGS/DeliveryZones/DeliveryZone.js" />	<script src="/Assets/Common/Js/ToolTips/cas_tips.js" type="text/javascript" />	<script type="text/javascript">		var globalZone = '';		var globalFee = '';		function AddDeliveryFeeToOrderWithDisplay(orderid, zone, deliveryfee){		var requestArgs = GSNContext.RequestArguments;		var url = document.URL;		globalZone = zone;		globalFee = deliveryfee;		GetDeliveryZoneAddDeliveryFee(url, requestArgs, orderid, deliveryfee, WindowClose, null);		}		function WindowClose(){		if(globalZone != '' &amp;&amp; globalFee != ''){				parent.DisplayDeliveryFee(globalZone, globalFee);			}						globalZone = '';			globalFee = '';		}	</script>		<div style="margin-left: auto; margin-right: auto;">		<h3>Click on your delivery destination from the map below.</h3>		<br />		<img>			<xsl:attribute name="id"><xsl:text>zonemap</xsl:text></xsl:attribute>			<xsl:attribute name="name">zonemap</xsl:attribute>			<xsl:attribute name="src">/assets/184/images/festfoods_deliveryzone.jpg</xsl:attribute>			<xsl:attribute name="usemap"><xsl:text>#m_zonemap</xsl:text></xsl:attribute>		</img> 	</div>	<div style="display: none; visibility: hidden;">		<xsl:for-each select="//DeliveryZones/Zone">			<span>				<xsl:attribute name="id">					<xsl:value-of select="concat(Title, 'zone')" />				</xsl:attribute>								<xsl:value-of select="Title" />: <xsl:value-of select="format-number(DeliveryFee, '$0.00')" />			</span>		</xsl:for-each>	</div>		<xsl:variable name="href">		<xsl:text>javascript:void(0)</xsl:text>	</xsl:variable>		<map name="m_zonemap" id="m_zonemap">		<area>			<xsl:attribute name="shape">poly</xsl:attribute>			<xsl:attribute name="coords">42,326,44,183,297,161,266,201,310,248,354,268,379,274,405,273,427,260,471,186,479,157,574,153,574,410,499,488,464,491,454,473,377,430,342,430,264,384,159,341,127,343,106,334,42,326</xsl:attribute>			<xsl:attribute name="href">				<xsl:value-of select="$href" />			</xsl:attribute>			<xsl:attribute name="alt"></xsl:attribute>			<xsl:attribute name="onmouseover">doTipFromTag(this, 'Green Bay Eastzone')</xsl:attribute>			<xsl:attribute name="onclick">				<xsl:call-template name="DeliveryJS">					<xsl:with-param name="zone" select="'Green Bay East'" />				</xsl:call-template>			</xsl:attribute>		</area>		<area>			<xsl:attribute name="shape">poly</xsl:attribute>			<xsl:attribute name="coords">44,596,41,328,104,337,126,346,158,343,264,387,340,431,378,433,453,475,463,492,327,504,70,599,44,596</xsl:attribute>			<xsl:attribute name="href">				<xsl:value-of select="$href" />			</xsl:attribute>			<xsl:attribute name="alt"></xsl:attribute>			<xsl:attribute name="onmouseover">doTipFromTag(this, 'De Perezone')</xsl:attribute>			<xsl:attribute name="onclick">				<xsl:call-template name="DeliveryJS">					<xsl:with-param name="zone" select="'De Pere'" />				</xsl:call-template>			</xsl:attribute>		</area>		<!--Green bay (yellow)		<area shape="poly" coords="42,326,44,183,297,161,266,201,310,248,354,268,379,274,405,273,427,260,471,186,479,157,574,153,574,410,499,488,464,491,454,473,377,430,342,430,264,384,159,341,127,343,106,334,42,326" href="javascript:;" alt="Green bay (yellow)" /> -->		<!--Depere (green) 		<area shape="poly" coords="44,596,41,328,104,337,126,346,158,343,264,387,340,431,378,433,453,475,463,492,327,504,70,599,44,596" href="javascript:;" alt="Depere (green)" />-->	</map></xsl:template><xsl:template name="DeliveryJS">	<xsl:param name="zone" />	<xsl:for-each select="//DeliveryZones/Zone">		<xsl:if test="Title = $zone">			<xsl:value-of select="concat('AddDeliveryFeeToOrderWithDisplay(', //Order/OrderID, ',', '&quot;', Title,'&quot;', ',', DeliveryFee, ');')" />				</xsl:if>	</xsl:for-each>	</xsl:template></xsl:stylesheet><!-- Stylus Studio meta-information - (c) 2004-2009. Progress Software Corporation. All rights reserved.
