<?xml version='1.0'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:output method="html" />
	<xsl:template match="/">
		<xsl:choose>
			<xsl:when test="//Email/IsSent = 'true'">
				<p>Your <xsl:value-of select="//Chain/ChainName"/> password has been sent to your registered email address.<br/>
                You should receive your information shortly.</p>
			</xsl:when>
			<xsl:when test="//Consumer">
				<p>We were unable to send your password to your registered email address at this time.</p>
			</xsl:when>
			<xsl:otherwise>
				<p>We were unable to find a match for the username or email address you specified.</p>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>
