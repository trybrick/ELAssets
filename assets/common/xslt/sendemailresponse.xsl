<?xml version='1.0'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:output method="html" />
	<xsl:template match="/">
		<xsl:choose>
			<xsl:when test="//Email/IsSent = 'true'">
				<h2>Your Email Has Been Sent!</h2>
			</xsl:when>
			<xsl:otherwise>
				<h2>Your Email Could Not Be Sent.</h2>
				<p>
					<xsl:value-of select="//Email/StatusMessage" />
				</p>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>
