<?xml version='1.0' encoding='utf-8'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:output method="html"/>

<xsl:template match="/">
	<a>
		<xsl:attribute name="href">
			##root##/Recipes/RecipeVideos.aspx
		</xsl:attribute>
		<img border="0">
			<xsl:attribute name="src">
				<xsl:value-of select="ExpressLane/RecipeVideoTeaser/Image"/>
			</xsl:attribute>
		</img>
	</a>
</xsl:template>

</xsl:stylesheet>
