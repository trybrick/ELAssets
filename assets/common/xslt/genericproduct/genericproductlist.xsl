<?xml version='1.0'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:output method="html" />
	<xsl:template match="/">
		<div>
			<table>
				<tbody>
					<xsl:for-each select="ArrayOfGenericProduct/GenericProduct">
						<tr>
							
								<td><a><xsl:value-of select="ProductID" /></a></td>
								<td><a><xsl:value-of select="Description" /></a></td>
								<td><a><xsl:value-of select="BrandName" /></a></td>
								<td><a><xsl:value-of select="SalePrice" /></a></td>
								<td><a><xsl:value-of select="SaleStartDate" /></a></td>
								
						</tr>
					</xsl:for-each>
				</tbody>
			</table>
		</div>
		
	</xsl:template>
