<?xml version='1.0'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:output method="html" />
	<xsl:template match="/">
		<xsl:choose>
			<xsl:when test="//ProfileEmail/Email/IsSent = 'true'">
				<p>Your username has been sent to your registered email address.<br/>
                You should receive it shortly.</p>
			</xsl:when>
			<xsl:when test="//ProfileEmail/Consumer">
				<p>We were unable to send you username to your registered email address at this time.</p>
			</xsl:when>
			<xsl:otherwise>
				<p>We were unable to find a match for the email address you have specified.</p>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>
